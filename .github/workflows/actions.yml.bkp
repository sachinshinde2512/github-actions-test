name: Workflow Actions

on: [push]

jobs: 
    run-github-actions:
      runs-on: ubuntu-latest
      steps: 
        - name: CheckOut
          id: checkout
          uses: actions/checkout@v3
        - name: List Files Before CheckOut 
          id: list2
          run: |
            pwd
            ls -a
            echo $GITHUB_REPOSITORY
            echo $GITHUB_SHA
            echo $GITHUB_WORKSPACE
            echo "${{ github.token }}"
            